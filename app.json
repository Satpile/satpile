{
  "expo": {
    "name": "Satpile",
    "slug": "satpile",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.satpile.app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "locales": {
      "fr": "./translations/permissions/fr.json",
      "en": "./translations/permissions/en.json"
    },
    "android": {
      "package": "com.satpile.app"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000",
      "translucent": false
    },
    "notification": {
      "iosDisplayInForeground": true,
      "icon": "./assets/icon_notification.png",
      "color": "#f47c1c",
      "androidCollapsedTitle": "Satpile"
    }
  }
}
